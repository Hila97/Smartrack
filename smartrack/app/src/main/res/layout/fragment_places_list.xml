<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.planTrip.PlacesListFragment">

    <ListView
        android:id="@+id/fragment_places_list_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="50dp"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="167dp" />
    <Button
        android:id="@+id/fragment_places_list_planBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lets plan tracks"
        android:textColor="@color/black"
        app:backgroundTint="@color/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_places_list_listview"/>
</androidx.constraintlayout.widget.ConstraintLayout>